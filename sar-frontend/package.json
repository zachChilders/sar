{
  "name": "sar-frontend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently -n gen,vite -c magenta,cyan \"npm run dev:gen\"  \"npm run dev:vite\"",
    "dev:vite": "vite",
    "build": "tsc && vite build",
    "dev:gen": "nodemon --watch ../sar-backend/openapi.json --exec npm run gen",
    "gen": "cp ../sar-backend/openapi.json src/services && rtk-query-codegen-openapi src/services/openApiConfig.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@reduxjs/toolkit": "^1.9.7",
    "@rtk-query/codegen-openapi": "^1.2.0",
    "framer-motion": "^10.17.9",
    "prettier": "^3.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.21.1",
    "ts-pattern": "^5.0.6",
    "vite-plugin-checker": "^0.6.2",
    "vite-tsconfig-paths": "^4.2.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
